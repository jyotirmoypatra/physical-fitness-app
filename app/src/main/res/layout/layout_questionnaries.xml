                    android:layout_marginTop="10dp"
                    android:text="Our goal is to decrease your score over time."
                    android:textColor="#333333"
                    android:textSize="@dimen/fab_fourteen" />
            </LinearLayout>
        </RelativeLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight=".3"
            android:orientation="horizontal">

            <View
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_weight=".01"
                android:background="#F2F2F2"
                android:visibility="gone"></View>

            <RelativeLayout
                android:id="@+id/rlResultDas"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight=".49"
                android:padding="5dp">

                <ImageView
                    android:visibility="gone"
                    android:id="@+id/imgMIconD"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"
                    android:src="@drawable/mbhq_eye" />

                <com.ashysystem.mbhq.view.TextViewOswaldLight
                    android:id="@+id/txtMIconD"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imgMIconD"
