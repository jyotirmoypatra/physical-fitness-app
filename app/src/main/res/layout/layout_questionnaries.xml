                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/imgMIconE"
                                android:layout_marginTop="5dp"
                                android:gravity="center"
                                android:text="SEE RESULT"
                                android:textColor="#00AE29"
                                android:textSize="@dimen/fab_eight"
                                android:visibility="gone" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/rlHappyAction"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_weight=".49"
                            android:padding="5dp">

                            <ImageView
                                android:id="@+id/imgEIconE"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="10dp"
                                android:src="@drawable/ic_add_green" />

                            <com.ashysystem.mbhq.view.TextViewOswaldLight
                                android:id="@+id/txtEIconE"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/imgEIconE"
                                android:layout_marginTop="5dp"
                                android:gravity="center"
                                android:text=""
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/fab_eight" />

                        </RelativeLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_happy_details"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/llHappyBefore"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight=".5"
                        android:orientation="vertical"
                        android:visibility="invisible">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            >
                            <!--android:background="@drawable/circle_light_green"-->
                            <com.ashysystem.mbhq.view.TextViewRalewayMedium
                                android:id="@+id/txtHappyBefore"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:layout_marginTop="5dp"
                                android:text="0"
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/fab_twenty"></com.ashysystem.mbhq.view.TextViewRalewayMedium>
                        </RelativeLayout>

                        <com.ashysystem.mbhq.view.TextViewSemiBold
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="10dp"
                            android:textColor="@color/black"
                            android:text="BEFORE"></com.ashysystem.mbhq.view.TextViewSemiBold>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/llHappyAfter"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight=".5"
                        android:orientation="vertical"
                        android:visibility="invisible">

                        <RelativeLayout
                            android:id="@+id/rlTypeNowHappy"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                           >
                           <!-- android:background="@drawable/circle_light_green"-->
                            <com.ashysystem.mbhq.view.TextViewRalewayMedium
                                android:id="@+id/txtHappyNow"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:layout_marginTop="5dp"
                                android:text="0"
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/fab_twenty"></com.ashysystem.mbhq.view.TextViewRalewayMedium>
                        </RelativeLayout>

                        <com.ashysystem.mbhq.view.TextViewSemiBold
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="10dp"
                            android:textColor="@color/black"
                            android:text="NOW"></com.ashysystem.mbhq.view.TextViewSemiBold>

                    </LinearLayout>

                    <ImageView
                        android:id="@+id/imgHappyAdd"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:background="@drawable/ic_add_mbhq_green_plus"
                        android:visibility="gone"></ImageView>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llRahe"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:background="@color/white"
                android:orientation="horizontal"
                android:visibility="gone">

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight=".6"
                    android:orientation="vertical"
                    android:padding="10dp">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:orientation="vertical">

                        <com.ashysystem.mbhq.view.TextViewOswaldRegular
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Rahe Perceived Stress"
                            android:textColor="#262324"
                            android:textSize="@dimen/fab_fourteen" />

                        <com.ashysystem.mbhq.view.TextViewOswaldRegular
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:text="STRESS"
                            android:textColor="@color/colorPrimary"
                            android:textSize="@dimen/fab_fourteen" />
                    </LinearLayout>
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight=".4"
                    android:orientation="horizontal">

                    <View
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="5dp"
                        android:layout_weight=".01"
                        android:background="#F2F2F2"></View>

                    <RelativeLayout
                        android:id="@+id/rlResultRahe"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight=".49"
                        android:padding="5dp">

                        <ImageView
                            android:id="@+id/imgMIconM"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="10dp"
                            android:src="@drawable/resulthdpi" />

                        <com.ashysystem.mbhq.view.TextViewOswaldLight
                            android:id="@+id/txtMIconM"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/imgMIconM"
                            android:layout_marginTop="5dp"
                            android:gravity="center"
                            android:text="SEE RESULT"
                            android:textColor="#00AE29"
                            android:textSize="@dimen/fab_eight" />

                    </RelativeLayout>

                    <View
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="5dp"
                        android:layout_weight=".01"
                        android:background="#F2F2F2"></View>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_weight=".49"
                        android:padding="5dp">

                        <ImageView
                            android:id="@+id/imgEIconM"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="10dp" />

                        <com.ashysystem.mbhq.view.TextViewOswaldLight
                            android:id="@+id/txtEIconM"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/imgEIconM"
                            android:layout_marginTop="5dp"
                            android:gravity="center"
                            android:text=""
                            android:textColor="@color/colorPrimary"
                            android:textSize="@dimen/fab_eight" />

                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>


<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="5dp"
    android:orientation="vertical"
    android:background="@color/white">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight=".7"
            android:orientation="vertical"
            android:padding="10dp">

            <LinearLayout
                android:id="@+id/llDas1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:orientation="vertical">

                <com.ashysystem.mbhq.view.TextViewBold
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="DAS TEST"
                    android:textColor="@color/black"
                    android:textSize="@dimen/fab_sixteen" />

                <com.ashysystem.mbhq.view.TextViewSemiBold
