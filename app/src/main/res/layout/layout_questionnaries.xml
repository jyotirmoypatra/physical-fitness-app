<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F2F2F2">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:id="@+id/rlHeader"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/mbhq_bar"
            android:visibility="gone">

            <com.ashysystem.mbhq.view.TextViewOswaldLight
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="QUESTIONNARIES"
                android:textColor="@color/white"
                android:textSize="@dimen/fab_sixteen" />
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/llMainBlock"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/rlHeader"
            android:layout_margin="10dp"
            android:orientation="vertical"
            android:visibility="visible">

            <LinearLayout
                android:id="@+id/llCohen"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:background="@color/white"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight=".7"
                        android:orientation="vertical"
                        android:padding="10dp">

                        <LinearLayout
                            android:id="@+id/ll_stress_details1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:orientation="vertical">

                            <com.ashysystem.mbhq.view.TextViewBold
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="STRESS TEST"
                                android:textColor="@color/black"
                                android:textSize="@dimen/fab_sixteen" />

                            <com.ashysystem.mbhq.view.TextViewSemiBold
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:text="Our goal is to reduce your score overtime"
                                android:textColor="#333333"
                                android:textSize="@dimen/fab_fourteen" />
                        </LinearLayout>
                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight=".3"
                        android:orientation="horizontal">

                        <View
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="5dp"
                            android:layout_weight=".01"
                            android:background="#F2F2F2"
                            android:visibility="gone"></View>

                        <RelativeLayout
                            android:id="@+id/rlResultCohen"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight=".49"
                            android:padding="5dp">

                            <ImageView
                                android:id="@+id/imgMIconT"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="10dp"
                                android:visibility="gone"
                                android:src="@drawable/mbhq_eye" />

                            <com.ashysystem.mbhq.view.TextViewOswaldLight
                                android:id="@+id/txtMIconT"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/imgMIconT"
                                android:layout_marginTop="5dp"
                                android:gravity="center"
                                android:text="SEE RESULT"
                                android:textColor="#00AE29"
                                android:textSize="@dimen/fab_eight"
                                android:visibility="gone" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/rlStressAction"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_weight=".49"
                            android:padding="5dp">

                            <ImageView
                                android:id="@+id/imgEIconT"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="10dp"
                                android:src="@drawable/progresshdpi" />

                            <com.ashysystem.mbhq.view.TextViewOswaldLight
                                android:id="@+id/txtEIconT"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/imgEIconT"
                                android:layout_marginTop="5dp"
                                android:gravity="center"
                                android:text=""
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/fab_eight" />

                        </RelativeLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_stress_details"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/llStressBefore"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight=".5"
                        android:orientation="vertical"
                        android:visibility="invisible">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                           >
                           <!-- android:background="@drawable/circle_light_green"-->
                            <com.ashysystem.mbhq.view.TextViewRalewayMedium
                                android:id="@+id/txtCohenBefore"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:text="0"
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/fab_twenty"></com.ashysystem.mbhq.view.TextViewRalewayMedium>
                        </RelativeLayout>

                        <com.ashysystem.mbhq.view.TextViewSemiBold
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="10dp"
                            android:textColor="@color/black"
                            android:text="BEFORE"></com.ashysystem.mbhq.view.TextViewSemiBold>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/llStressAfter"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight=".5"
                        android:orientation="vertical"
                        android:visibility="invisible">

                        <RelativeLayout
                            android:id="@+id/rlTypeNowStress"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                           >
                           <!-- android:background="@drawable/circle_light_green"-->
                            <com.ashysystem.mbhq.view.TextViewRalewayMedium
                                android:id="@+id/txtCohenNow"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:text="0"
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/fab_twenty"></com.ashysystem.mbhq.view.TextViewRalewayMedium>
                        </RelativeLayout>

                        <com.ashysystem.mbhq.view.TextViewSemiBold

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="10dp"
                            android:textColor="@color/black"
                            android:text="NOW"></com.ashysystem.mbhq.view.TextViewSemiBold>

                    </LinearLayout>

                    <ImageView
                        android:id="@+id/imgStressAdd"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:background="@drawable/ic_add_mbhq_green_plus"
                        android:visibility="gone"></ImageView>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
