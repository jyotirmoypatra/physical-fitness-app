<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <RelativeLayout
        android:id="@+id/rlTop"
        android:layout_width="match_parent"
        android:layout_height="@dimen/action_bar_height"
        android:background="#00000000">

    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/rlTop"
        android:background="#B3000000">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight=".75"
            android:orientation="vertical"
            android:background="@color/white">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp">
                <com.ashysystem.mbhq.view.TextViewRalewayMedium
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/fab_sixteen"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:text="filter"
                    android:textColor="@color/mbhq_base_color"
                    android:textAllCaps="true"/>
                <ImageView
                    android:id="@+id/imgClose"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="15dp"
                    android:layout_centerVertical="true"
                    android:background="@drawable/mbhq_close"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:visibility="gone">
                <com.ashysystem.mbhq.view.TextViewRalewayMedium
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/fab_sixteen"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:text="date"
                    android:textColor="@color/black"
                    android:textAllCaps="true"/>
                <ImageView
                    android:id="@+id/imgExpandClose"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="15dp"
                    android:layout_centerVertical="true"
                    android:visibility="gone"
                    android:background="@drawable/mbhq_expand_less"/>
            </RelativeLayout>
            <LinearLayout
                android:id="@+id/llCheckBoxOptions"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <RelativeLayout
                    android:id="@+id/rlShowAll"
                    android:layout_width="match_parent"
                    android:layout_height="45dp">
                    <ImageView
                        android:id="@+id/imgShowAllCheck"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dp"
                        android:layout_centerVertical="true"
                        android:background="@drawable/mbhq_ic_tick"/>
                    <com.ashysystem.mbhq.view.TextViewRalewayMedium
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/fab_fourteen"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15dp"
                        android:text="monthly view"
                        android:layout_toRightOf="@+id/imgShowAllCheck"
                        android:textColor="@color/black"
                        android:textAllCaps="true"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rlToday"
                    android:layout_width="match_parent"
                    android:layout_height="45dp">
                    <ImageView
                        android:id="@+id/imgTodayCheck"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15dp"
                        android:background="@drawable/mbhq_ic_untick" />
                    <com.ashysystem.mbhq.view.TextViewRalewayMedium
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/fab_fourteen"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15dp"
                        android:text="quarterly view"
                        android:layout_toRightOf="@+id/imgTodayCheck"
                        android:textColor="@color/black"
                        android:textAllCaps="true"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rlThisMonth"
                    android:layout_width="match_parent"
                    android:layout_height="45dp">
                    <ImageView
                        android:id="@+id/imgThisMonthCheck"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dp"
                        android:layout_centerVertical="true"
                        android:background="@drawable/mbhq_ic_untick"/>
                    <com.ashysystem.mbhq.view.TextViewRalewayMedium
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/fab_fourteen"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15dp"
                        android:text="yearly view"
                        android:layout_toRightOf="@+id/imgThisMonthCheck"
                        android:textColor="@color/black"
                        android:textAllCaps="true"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rlSixMonth"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:visibility="gone">
                    <ImageView
                        android:id="@+id/imgSixMonthCheck"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dp"
                        android:layout_centerVertical="true"
                        android:background="@drawable/mbhq_ic_untick"/>
                    <com.ashysystem.mbhq.view.TextViewRalewayMedium
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/fab_fourteen"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15dp"
                        android:text="active"
                        android:layout_toRightOf="@+id/imgSixMonthCheck"
                        android:textColor="@color/black"
                        android:textAllCaps="true"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rlOneYear"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:visibility="gone">
                    <ImageView
                        android:id="@+id/imgOneYearCheck"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dp"
                        android:layout_centerVertical="true"
                        android:background="@drawable/mbhq_ic_untick"/>
                    <com.ashysystem.mbhq.view.TextViewRalewayMedium
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/fab_fourteen"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15dp"
                        android:text="completed"
                        android:layout_toRightOf="@+id/imgOneYearCheck"
                        android:textColor="@color/black"
                        android:textAllCaps="true"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rlSelectDate"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:visibility="gone">
                    <ImageView
                        android:id="@+id/imgSelectDateRange"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dp"
                        android:layout_centerVertical="true"
                        android:background="@drawable/mbhq_ic_untick"/>
                    <com.ashysystem.mbhq.view.TextViewRalewayMedium
                        android:id="@+id/txtSelectDate"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/fab_fourteen"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15dp"
                        android:text="snooze/hidden"
                        android:layout_toRightOf="@+id/imgSelectDateRange"
                        android:textColor="@color/black"
                        android:textAllCaps="true"/>
                </RelativeLayout>
                <LinearLayout
                    android:id="@+id/llSecetDateWhole"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:weightSum="9"
                    android:visibility="gone">
                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="4"
                        android:background="@drawable/rounded_corner_mbhq_border"
                        >
                        <com.ashysystem.mbhq.view.TextViewRalewayMedium
                            android:id="@+id/txtFromDate"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_centerInParent="true"
                            android:textSize="@dimen/fab_fourteen"
                            android:textColor="@color/black"
                            android:textAllCaps="true"
                            android:text="from date"
                            android:gravity="center"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        >
                        <com.ashysystem.mbhq.view.TextViewRalewayMedium
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:textSize="@dimen/fab_fourteen"
                            android:textColor="@color/black"
                            android:textAllCaps="true"
                            android:text="to"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="4"
                        android:background="@drawable/rounded_corner_mbhq_border"
                        >
                        <com.ashysystem.mbhq.view.TextViewRalewayMedium
                            android:id="@+id/txtToDate"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_centerInParent="true"
                            android:textSize="@dimen/fab_fourteen"
                            android:textColor="@color/black"
                            android:textAllCaps="true"
                            android:text="to date"
                            android:gravity="center"/>
                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginBottom="20dp">
                    <RelativeLayout
                        android:id="@+id/rlShowResults"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight=".5"
                        android:layout_marginRight="5dp"
                        android:background="@drawable/rounded_corner_green">
                        <com.ashysystem.mbhq.view.TextViewRalewayMedium
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:textColor="@color/white"
                            android:textSize="@dimen/fab_twelve"
                            android:textAllCaps="true"
                            android:text="show results"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/rlClearAll"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight=".5"
                        android:layout_marginLeft="5dp"
                        android:background="@drawable/rounded_corner_green_border_white_inside">
                        <com.ashysystem.mbhq.view.TextViewRalewayMedium
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:textColor="@color/mbhq_base_color"
                            android:textSize="@dimen/fab_twelve"
                            android:textAllCaps="true"
                            android:text="clear all"/>
                    </RelativeLayout>

                </LinearLayout>
            </RelativeLayout>
        </LinearLayout>
        <RelativeLayout
            android:id="@+id/rlTransparent"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight=".25">

        </RelativeLayout>
    </LinearLayout>
</RelativeLayout>