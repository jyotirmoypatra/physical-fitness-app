                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/ashseparator"></View>
            <LinearLayout
                android:id="@+id/llIDreamtOf"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <com.ashysystem.mbhq.view.TextViewRaleWay
                    android:id="@+id/txtIDreamtOf"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:gravity="center_vertical"
                    android:paddingLeft="10dp"
                    android:text="I dreamt of"
                    android:textColor="@color/black"
                    android:textSize="@dimen/fab_sixteen" />
                 <ImageView
                     android:id="@+id/checkIDreamtOf"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginRight="5dp"
                        android:padding="8dp"
                        android:src="@drawable/mbhq_circle_green" />
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/ashseparator"></View>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/rlCancel"
        android:layout_width="match_parent"
        android:layout_height="@dimen/action_bar_height"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/rounded_corner_mbhq_filled">

        <com.ashysystem.mbhq.view.TextViewRaleWay
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="cancel"
            android:textAllCaps="true"
            android:textColor="@color/white"
            android:textSize="@dimen/fab_sixteen" />
    </RelativeLayout>
</RelativeLayout>