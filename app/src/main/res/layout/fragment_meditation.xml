                            android:layout_height="wrap_content"
                            android:layout_marginStart="30dp"
                            android:layout_marginEnd="30dp"
                            android:orientation="vertical"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent">


                            <RelativeLayout
                                android:id="@+id/rlGuidedMeditations"
                                android:layout_width="0dp"
                                android:layout_height="50dp"
                                android:background="@drawable/capsule_green"
                                android:visibility="visible"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintWidth_percent="1">

                                <com.ashysystem.mbhq.view.TextViewRalewayMedium
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerInParent="true"
                                    android:text="Guided Meditations"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/fab_eighteen" />
                            </RelativeLayout>

                            <RelativeLayout
                                android:id="@+id/rlVisualisations"
                                android:layout_width="0dp"
                                android:layout_height="50dp"
                                android:layout_marginTop="40dp"
                                android:background="@drawable/capsule_green"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/rlGuidedMeditations"
                                app:layout_constraintWidth_percent=".8">

                                <com.ashysystem.mbhq.view.TextViewRalewayMedium
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerInParent="true"
                                    android:text="Visualisations"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/fab_eighteen" />
                            </RelativeLayout>

                            <RelativeLayout
                                android:id="@+id/rlHealingMeditations"
                                android:layout_width="0dp"
                                android:layout_height="50dp"
                                android:layout_marginTop="40dp"
                                android:background="@drawable/capsule_green"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/rlVisualisations"
                                app:layout_constraintWidth_percent=".6">

                                <com.ashysystem.mbhq.view.TextViewRalewayMedium
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerInParent="true"
                                    android:text="Healing"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/fab_eighteen" />
                            </RelativeLayout>

                            <RelativeLayout
                                android:id="@+id/rlHealingsleep"
                                android:layout_width="0dp"
                                android:layout_height="50dp"
                                android:layout_marginTop="40dp"
                                android:background="@drawable/capsule_green"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/rlHealingMeditations"
                                app:layout_constraintWidth_percent=".6">

                                <com.ashysystem.mbhq.view.TextViewRalewayMedium
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerInParent="true"
                                    android:text="Sleep"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/fab_eighteen" />
                            </RelativeLayout>
                        </androidx.constraintlayout.widget.ConstraintLayout>

                    </androidx.constraintlayout.widget.ConstraintLayout>
                </LinearLayout>

            </RelativeLayout>


        </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>




        <FrameLayout
            android:id="@+id/frm_notaccess_meditation"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff"
            android:visibility="gone">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:weightSum="3"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/img_notaccess_meditation"
                    android:layout_width="wrap_content"
                    android:layout_weight="2.8"
                    android:layout_gravity="center"
                    android:layout_height="0dp"
                    android:src="@drawable/eq_access">

                </ImageView>
                <com.ashysystem.mbhq.view.ButtonOswaldRegular
                    android:id="@+id/txt_notaccess_meditation"
                    android:layout_width="match_parent"
                    android:layout_weight=".2"
                    android:layout_height="0dp"
                    android:layout_gravity="bottom"
                    android:gravity="center"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:textColor="#FC191717"
                    android:layout_marginBottom="60dp"
                    android:text="I’ve upgraded, update my access please">

                </com.ashysystem.mbhq.view.ButtonOswaldRegular>

            </LinearLayout>

        </FrameLayout>

    </FrameLayout>


